    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-tab-view": "3.5.0",
    "react-native-web": "0.18.12",
    "react-native-webview": "^13.6.4",
    "react-native-windows": "0.73.2",
    "react-native-winrt": "0.72"
